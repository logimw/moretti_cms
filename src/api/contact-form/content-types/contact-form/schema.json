{
  "kind": "singleType",
  "collectionName": "contact_forms",
  "info": {
    "singularName": "contact-form",
    "pluralName": "contact-forms",
    "displayName": "Formularz kontaktowy",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "template": {
      "type": "richtext"
    },
    "subject": {
      "type": "string"
    },
    "host": {
      "type": "string"
    },
    "port": {
      "type": "string",
      "maxLength": 3,
      "minLength": 3,
      "required": true,
      "default": "465"
    },
    "hostPassword": {
      "type": "password",
      "required": true
    },
    "hostEmail": {
      "type": "email",
      "required": true
    },
    "receiveEmails": {
      "type": "component",
      "repeatable": true,
      "component": "form.email",
      "required": true
    },
    "messageOnSuccess": {
      "type": "string"
    }
  }
}
